{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@layouts": ["src/layouts"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
